{
  "env": {
    "es6": true,
    "browser": true
  },
  "parser":
    "babel-eslint",
  "parserOptions": {
    "sourceType":
      "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": [ "warn", { "args": "none" } ],
    "react/prop-types": "warn"
  }
}
